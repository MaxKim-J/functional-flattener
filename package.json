    "jest": "^26.6.3",
    "@types/jest": "^26.0.16",
    "eslint-plugin-import": "^2.22.1",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "@types/lodash.camelcase": "^4.3.6",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/lodash.snakecase": "^4.1.6",
    "lodash.camelcase": "^4.3.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.snakecase": "^4.1.1"
